<view class="cont">
  <view class="top-time">
    <picker mode="date">
      <view class="tip right-img">
        <text>{{date}}</text>
        <image src="/img/right.png"></image>
      </view>
    </picker>
  </view>
  <view class="cont-top">
    <label>
      <text>访客数（人）</text>
      <text>100</text>
    </label>
    <label>
      <text>访客数（人）</text>
      <text>100</text>
    </label>
    <label>
      <text>访客数（人）</text>
      <text>10</text>
    </label>
  </view>
  <block wx:if='{{dataList.length != 0}}'>
    <view class="list" wx:for="{{dataList}}" wx:key="list" bindtap="toDay" id="{{index}}">
      <view>
        <view class="date">
          <image src="/img/index/wechat.png" class="img"></image>
          <view>{{item.date}}</view>
        </view>
        <view class="pay-list">
          <view class="label-num label">旺座现代城店</view>
        </view>
      </view>
      <view class="money">{{item.orderMoney}}</view>
    </view>
  </block>
  <block wx:if="{{dataList.length==0}}">
    <v-info fromimages="{{images}}" frominfoWord="{{infoWord}}" formheight="{{height}}"></v-info>
  </block>
</view>